#include<iostream>
#include<cstring>
#include<algorithm>
using namespace std;
char s[100][256];
int main() {
	int n,minLen = 256;
	int ans = 0;
	scanf("%d", &n);
	getchar();
	for (int i = 0; i < n; i++) {
		cin>>str[i];
		int len = strlen(s[i]);
		if (len < minLen) minLen = len;
		for (int j = 0; j < len / 2; j++) {
			char temp = s[i][j];
			s[i][j] = s[i][len - j - 1];
			s[i][len - j - 1];
		}
	}
	for (int i = 0; i < minLen; i++) {
		char c = s[0][i];
		bool same = true;
		
		for (int j = 1; j < n; j++) {
			if (c != s[j][i]) {
				same = false;
				break;
			}
		}
		if (same == true) {
			ans++;
		}
		else break;
	}
	if (ans!=0) {
		for (int i = ans - 1; i >= 0; i--) {
			printf("%c", s[0][i]);
		}
	}
	else {
		printf("nai");
	}
	return 0;
}
