#include<cstdio>
const int maxn = 50010;
int n;
int pre[maxn], in[maxn];
struct node {
	int data;
	node* lchild;
	node* rchild;
	node() {
		node* lchild =NULL;
		node* rchild = NULL;
	}
};
node* create(int prel, int prer, int inl, int inr) {
	if (prel > prer) {
		return NULL;
	}
	node* root = new node;
	root->data = pre[prel];
	int k;
	for (k = inl; k <= inr; k++) {
		if (in[k] == pre[prel]) {
			break;
		}
	}
	int numLeft = k - inl;
	root->lchild = create(prel + 1, prel + numLeft, inl, k - 1);
	root->rchild = create(prel + numLeft + 1, prer, k + 1, inr);
	return root;
}
int main() {
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf("%d", &pre[i]);
	}
	for (int i = 0; i < n; i++) {
		scanf("%d", &pre[i]);
	}
	node* root = create(0, n - 1, 0, n - 1);

	while (root->lchild != NULL) {
		root = root->lchild;
		
	}
	int ans = root->data;
	printf("%d\n", ans);
	return 0;
}
