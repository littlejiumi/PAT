#include<cstdio>
const int Galleon = 17 * 29;
const int Sickle = 29;

int main() {
	int a1, a2, a3, b1, b2, b3;
	scanf("%d.%d.%d %d.%d.%d", &a1, &a2, &a3, &b1, &b2, &b3);
	int a = a1 * Galleon + a2 * Sickle + a3;
	int b = b1 * Galleon + b2 * Sickle + b3;
	int c = a + b;
	printf("%d.%d.%d", c / Galleon, c % Galleon / Sickle, c % Sickle);
	return 0;
}
