#include<cstdio>
struct Person{
	char name[15];
	char ID[15];
	int grade;
}M, F, temp;

void init() {
	M.grade = 101;
	F.grade = -1;
}
int main() {
	int n ;
	char gender;
	scanf("%d", &n);
	init();
	for(int i = 0; i<n; i++) {
		scanf("%s %c %s %d", &temp.name, &gender, &temp.ID, &temp.grade );
		if(gender == 'M' && temp.grade < M.grade ) {
			M = temp;
		}
		if(gender == 'F' && temp.grade > F.grade ) {
			F = temp;
		}
	}
	if(F.grade == -1){
		printf("Absent\n");
	}else{
		printf("%s %s\n", F.name, F.ID);
	}
	if(M.grade == 101){
		printf("Absent\n");
	}else{
		printf("%s %s\n", M.name, M.ID);
	}
	
	if(M.grade == 101 || F.grade == -1){
		printf("NA\n");
	}else{
		printf("%d\n", F.grade - M.grade );
	}
	return 0;
}
