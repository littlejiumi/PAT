#include<cstdio>
#include<cstring>
using namespace std;
char name[5];
int Friend[40] = { 0 };
int main() {
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		scanf("%s", &name);
		int s = strlen(name);
		int ans = 0;
		for (int j = 0; j < s; j++) {
			ans += (name[j] - '0');
		}
		Friend[ans]++;		
	}

	int count = 0;
	for (int i = 0; i<40; i++) {
		if (Friend[i] >= 1) {
			count++;
		}
	}
	printf("%d\n", count);
	for (int i = 0; i < 40; i++) {
		if (Friend[i] >= 1 && count > 1) {
			printf("%d ", i);
			count--;
		}
		else if (Friend[i] >= 1 && count == 1) {
			printf("%d", i);
		}
	}

	return 0;
}
