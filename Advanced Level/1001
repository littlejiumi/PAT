#include<cstdio>
#include<cstring>
#include<iostream>
int main() {
	int a, b, num = 0;
	char ans[15];
	scanf("%d%d", &a, &b);
	int sum = a + b;
	if (sum < 0) {
		printf("-");
		sum = -sum;
	}
	if(sum == 0) {
	  ans[0] = '0';
	  num = 1;
	}
	while (sum != 0) {
		ans[num++] = '0' + sum % 10;
		sum /= 10;
	}
	for (int i = num - 1; i >= 0; i--) {
		printf("%c", ans[i]);
		if (i % 3 == 0 && i > 0)printf(",");
	}
	return 0;
}
